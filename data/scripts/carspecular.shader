// Hotrod specular
progs/HotRod_body.mdl_0
{
    {
		program defaultskin
		map progs/HotRod_body
		rgbGen lightingDiffuse // proper shading for vertex lit models, or something. 
    }		
	{
		map progs/carspec
		blendFunc GL_SRC_ALPHA GL_ONE
		alphaGen const 0.06
		tcGen environment
		rgbgen entity
	}
}

